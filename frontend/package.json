    "lint": "eslint .",
    "gen:api": "openapi-ts && prettier --write src/api/generated",
    "gen:api:watch": "openapi-ts --watch",
    "preview": "vite preview",
    "cap:sync": "cap sync",
    "android:list": "cap run android --list",
    "android:run": "cap run android",
    "android:run:lr": "bash -lc 'JAVA_HOME=$(/usr/libexec/java_home -v 21 2>/dev/null || true); if [ -z \"$JAVA_HOME\" ]; then echo \"JDK 21 not found. Install JDK 21 (e.g. brew install openjdk@21) and try again.\"; exit 1; fi; export JAVA_HOME; export PATH=\"$JAVA_HOME/bin:$PATH\"; cap run android -l --host 127.0.0.1 --port 5173 --forwardPorts 5173:5173 --target RZCWC0YPZ2W --no-sync'"
